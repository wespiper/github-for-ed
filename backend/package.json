{
  "name": "scribe-tree-backend",
  "version": "1.0.0",
  "description": "Scribe Tree educational writing platform backend",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "dev:distributed": "ts-node src/scripts/init-distributed-system.ts --verbose",
